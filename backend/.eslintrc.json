{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["temp.js", "/root/build/static/*.js"],
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "import/no-unresolved":"off",
        "use-isnan":"off"
    }
}
